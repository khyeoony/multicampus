/* 학습내용
 * 1. 조건식 
 * 2. 문법
 * 		1. 조건 상황이 하나인 경우
 * 				if(조건식){}						1번
 * 		2. 조건 상황이 다수인 경우
 * 				if(조건식){						1번
 * 				}else if(조건식2){		0~*
 * 				}else if(조건식3){	
 * 				}else{모든 조건식에 해당되지 않을 경우}		1번
 * 		3. 조건 상황들이 아닌 경우도 고려해야 하는 경우
 * 				if(){}else{}
 * 
 * if(조건식){}
 * if(조건식){}
 * if(조건식){} // 개별적으로 진행
 * 
 * if(조건식){
 * else if(조건식) }
 * else if(조건식) } // if가 맞으면 넘어가버림
 * }
 */




package step01.syntax;

public class Step09If {
	public static char getGrade(String value) {
		char retVal = 'A';//초기화 해주는 것
		int value2 = Integer.parseInt(value); //String을 int로 바꾸기 위해 value2라는 변수를 새로 만듬
		
		if(value2 >=90) {
			retVal = 'A';
		}else if(value2 >= 80) {
			retVal = 'B';
		}else if(value2 >= 70) {
			retVal = 'C';
		}else {
			retVal = 'F';
		}	
	
		return retVal; 
	}

	public static void main(String[] args) {
		System.out.println(getGrade(91));
		System.out.println(getGrade(51));
		System.out.println(getGrade("50"));
		
		
	}

}
